    <script>
      $(function () {

      var user = {{{ json data.user }}};

      $('.wishlist-product-remove').click(function () {
        if (!user) {
          alert('please login first');
          return;
        }
        var _thisElm = $(this);
        var productSlug = _thisElm.attr('data-wishlist-slug');

        $.ajax({
          type: 'POST',
          url: '/wishlist/itemDelete',
          contentType: "application/json",
          data: JSON.stringify({
            slug: productSlug,
          }),
          success: function (data) {
            window.location.reload();
          }
        });
      });

      $('#registrationForm').submit(function (event) {
        event.preventDefault();

        $.ajax({
          type: 'POST',
          url: `/emailRegisteration`,
          contentType: "application/json",
          data: JSON.stringify({
            name: $('#registrationUserName').val(),
            email: $('#registrationUserEmail').val(),
            password: $('#registrationUserPassword').val(),
          }),
          success: function (data) {
            alert('verification link has been sent to your email!');
          },
          error: function (data) {
            $('#registration-failure-message').removeClass('hidden');
            $('#registration-failure-message').text(data.responseJSON.errorMessage || 'some error happened!');
            setTimeout(function () {
              $('#registration-failure-message').addClass('hidden');
            }, 10000);
          }
        });

      });

      $('#log-out').click(function () {
        $.ajax({
          type: 'POST',
          url: `/emailLogOut`,
          contentType: "application/json",
          data: JSON.stringify({}),
          success: function (data) {
            window.location.reload();
          },
          error: function (data) {

          }
        });
      });


      $('#loginForm').submit(function (event) {
        event.preventDefault();

        $.ajax({
          type: 'POST',
          url: `/emailLogIn`,
          contentType: "application/json",
          data: JSON.stringify({
            email: $('#loginUserEmail').val(),
            password: $('#loginUserPassword').val(),
          }),
          success: function (data) {
            window.location.reload();
          },
          error: function (data) {
            $('#login-failure-message').removeClass('hidden');
            $('#login-failure-message').text(data.responseJSON.errorMessage || 'some error happened!');
            setTimeout(function () {
              $('#login-failure-message').addClass('hidden');
            }, 10000);
          }
        });

      });

      $('#userForgottenPassword').submit(function (event) {
        event.preventDefault();

        $.ajax({
          type: 'POST',
          url: `/resetEmailForgottenPassword`,
          contentType: "application/json",
          data: JSON.stringify({
            email: $('#userForgottenEmail').val(),
          }),
          success: function (data) {
            $('#userForgottenPasswordReset').removeClass('hidden');
            $('#userForgottenPassword').addClass('hidden');
            setTimeout(function () {
              $('#userForgottenPassword').removeClass('hidden');
              $('#userForgottenPasswordReset').addClass('hidden');
            }, 60 * 1000 * 5);
          },
          error: function (data) {
            $('#forgottenPasswordResponseMessage').text(data.responseJSON.errorMessage || 'some error happened!');
            $('#forgottenPasswordResponseMessage').removeClass('hidden');
            setTimeout(function () {
              $('#forgottenPasswordResponseMessage').addClass('hidden');
            }, 10000);
          }
        });

      });

      $('#userForgottenPasswordReset').submit(function (event) {
        event.preventDefault();

        $.ajax({
          type: 'POST',
          url: `/resetForgottenPassword`,
          contentType: "application/json",
          data: JSON.stringify({
            email: $('#userForgottenEmail').val(),
            resetCode: $('#userForgottenCode').val(),
            newPassword: $('#userForgottenNewPassword').val(),
          }),
          success: function (data) {
            $('#forgottenPasswordResetResponseMessage').text('your password has been sent');
            setTimeout(function () {
              window.location.reload();
            }, 3000);

          },
          error: function (data) {
            $('#forgottenPasswordResetResponseMessage').text(data.responseJSON.errorMessage || 'some error happened!');
            $('#forgottenPasswordResetResponseMessage').removeClass('hidden');
            setTimeout(function () {
              $('#forgottenPasswordResetResponseMessage').addClass('hidden');
            }, 10000);
          }
        });

      });

      $('.wishlistInactive').click(function () {
        alert('Please login first to save items in your whishlist');
      });
      
      });
    </script>